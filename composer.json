{
    "name": "marcandreappel/laravel-autoforms",
    "description": "Automatically generated forms through a form builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc-Andr√© Appel",
            "email": "marc-andre@appel.fun"
        }
    ],
    "homepage": "https://github.com/marcandreappel/laravelautoforms",
    "keywords": ["Laravel", "LaravelAutoForms"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "MarcAndreAppel\\LaravelAutoForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcAndreAppel\\LaravelAutoForms\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcAndreAppel\\LaravelAutoForms\\LaravelAutoFormsServiceProvider"
            ],
            "aliases": {
                "LaravelAutoForms": "MarcAndreAppel\\LaravelAutoForms\\Facades\\LaravelAutoForms"
            }
        }
    }
}
